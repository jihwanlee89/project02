<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.payment.mapper.CardCompanyMapper">
	<insert id="insertRequestData" parameterType="java.lang.String">
		INSERT INTO CARD_COMPANY_TRANS (
			REQUEST_DATA
		) VALUES (
			#{data}
		)
	</insert>
</mapper>